{
  "markdownlint.config": {
    "MD041": false
  },
  "typescript.tsc.autoDetect": "off",
  "editor.tabSize": 2,
  "standard.engine": "ts-standard",
  "standard.autoFixOnSave": true,
  "cSpell.ignoreWords": [
    "Funcs",
    "Ionescu",
    "Liviu",
    "POSIX",
    "Schlueter",
    "Stmts",
    "endianness",
    "liquidjs",
    "npmignore",
    "npmjs",
    "tmpdir",
    "xpack"
  ]
}
